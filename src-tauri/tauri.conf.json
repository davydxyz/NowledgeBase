{
  "$schema": "https://schema.tauri.app/config/2.0.json",
  "productName": "NowledgeBase",
  "version": "0.1.0",
  "identifier": "com.niubi.knowledgebase",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "NowledgeBase",
        "width": 1000,
        "height": 700,
        "resizable": true,
        "maximizable": true,
        "minimizable": true,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "decorations": true,
        "transparent": false,
        "center": true,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false
  }
}